{
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "files.associations": {
      "*.vue": "html"
    },
    "eslint.options": {
      "configFile": "./.eslintrc.json"
    },
    "eslint.validate": ["javascript", "vue", "html", "typescript"],
    "workbench.startupEditor": "none",
    "stylelint.validate": ["css", "scss"],
    "scss.validate": false,
    "css.validate": false
}